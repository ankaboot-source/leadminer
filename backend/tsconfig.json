{
  "compilerOptions": {
    "outDir": "dist",
    "target": "esnext",
    "allowJs": false,
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*", "test/**/*"]
}
