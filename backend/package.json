{
  "name": "leadminer.io",
  "version": "1.0.0",
  "description": "leadminer.io app",
  "main": "server.js",
  "scripts": {
    "test:unit": "nyc --reporter=text --reporter=json-summary --reporter=lcov mocha test/unit/*.test.js --config .mocharc.js",
    "test:integration": "nyc --reporter=text --reporter=json-summary --reporter=lcov mocha -r dotenv/config test/integration/*.test.js --config .mocharc.js --timeout 60000",
    "test": "nyc --reporter=text --reporter=json-summary --reporter=lcov mocha -r dotenv/config test/**/*.test.js --config .mocharc.js --exit",
    "dev": "nodemon server.js",
    "lint:fix": "cd app && eslint --fix .",
    "lint:report": "cd app && eslint --fix . --output-file eslint_report.json --format json",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier . --write",
    "make-badges": "istanbul-badges-readme --readmeDir='../' --linesLabel='Coverage'"
  },
  "keywords": [
    "nodejs",
    "express",
    "sequelize",
    "rest",
    "api",
    "postgresql"
  ],
  "author": "ankaboot",
  "license": "GNU AGPL v3",
  "dependencies": {
    "@sentry/node": "^7.13.0",
    "@sentry/tracing": "^7.13.0",
    "@supabase/supabase-js": "^2.1.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-sse": "github:omgimalexis/express-sse",
    "google-auth-library": "^8.5.2",
    "imap": "^0.8.19",
    "ioredis": "^5.2.2",
    "nodemon": "^2.0.15",
    "object-scan": "^17.1.0",
    "pg": "^8.8.0",
    "pg-format": "^1.0.4",
    "quoted-printable": "^1.0.1",
    "winston": "^3.6.0",
    "xoauth2": "^1.2.0"
  },
  "devDependencies": {
    "c8": "^7.11.0",
    "chai": "^4.3.7",
    "chai-spies": "^1.0.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-eqeqeq-fix": "^1.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-vue": "^8.5.0",
    "istanbul-badges-readme": "^1.8.2",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "prettier": "2.7.1",
    "recheck": "^4.4.1",
    "supertest": "^6.3.1"
  },
  "engines": {
    "node": ">=15.0.0"
  }
}
