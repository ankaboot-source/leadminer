{
  "name": "leadminer.io",
  "version": "1.0.0",
  "description": "leadminer.io app",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "test:unit": " mocha test/unit/**/*.test.js --config .mocharc.js",
    "test:integration": "mocha -r dotenv/config test/integration/*.test.js --config .mocharc.js --timeout 60000",
    "test-ci:unit": "nyc --reporter=json-summary --reporter=lcov npm run test:unit",
    "dev": "nodemon src/server.ts",
    "dev:inspect": "node --inspect server.js",
    "lint": "eslint src test",
    "lint:fix": "eslint --fix src test",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "update-providers": "node fetchProviders.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "sequelize",
    "rest",
    "api",
    "postgresql"
  ],
  "author": "ankaboot",
  "license": "GNU AGPL v3",
  "dependencies": {
    "@sentry/node": "^7.13.0",
    "@sentry/tracing": "^7.13.0",
    "@supabase/supabase-js": "^2.1.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-sse": "github:omgimalexis/express-sse",
    "generic-pool": "^3.9.0",
    "google-auth-library": "^8.5.2",
    "html-entities": "^2.3.3",
    "imap": "^0.8.19",
    "ioredis": "^5.2.2",
    "nanoid": "^3.3.4",
    "nodemon": "^2.0.15",
    "pg": "^8.8.0",
    "pg-format": "^1.0.4",
    "quoted-printable": "^1.0.1",
    "winston": "^3.6.0",
    "xoauth2": "^1.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.0",
    "@types/pg": "^8.6.6",
    "@types/pg-format": "^1.0.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "chai": "^4.3.7",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "prettier": "2.7.1",
    "recheck": "^4.4.1",
    "supertest": "^6.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=15.0.0"
  }
}
