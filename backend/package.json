    "html-entities": "^2.5.2",
    "imap": "^0.8.19",
    "imap-autoconfig": "^0.1.5",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^3.3.7",
    "nodemon": "^3.1.4",
    "pg": "^8.11.5",
    "pg-format": "^1.0.4",
    "qs": "^6.12.2",
    "quoted-printable": "^1.0.1",
    "simple-oauth2": "^5.0.1",
