{
  "name": "leadminer.io",
  "version": "1.0.0",
  "description": "leadminer.io app",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "test:unit": "jest ./test/unit",
    "test:integration": "jest ./test/integration",
    "test-ci:unit": "jest ./test/unit --coverage",
    "dev-api": "nodemon src/server.ts",
    "dev-mock-external-services": "nodemon src/mocks/enrichement/server.ts",
    "dev-worker": "nodemon src/messageWorker.ts",
    "dev-email-worker": "nodemon src/emailVerificationWorker.ts",
    "dev:webhook-listener": "stripe listen --forward-to http://localhost:8081/api/payment/stripe/webhook",
    "dev:inspect": "ts-node --inspect src/server.ts",
    "lint": "eslint src test",
    "lint:fix": "eslint --fix src test",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "update-providers": "ts-node src/fetchProviders.ts"
  },
  "keywords": [
    "nodejs",
    "express",
    "sequelize",
    "rest",
    "api",
    "postgresql"
  ],
